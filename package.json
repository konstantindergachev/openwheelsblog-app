{
  "name": "openwheelsblog-app",
  "version": "1.0.0",
  "description": "A simple search application in Node and Express hosted on Heroku",
  "main": "app/server.js",
  "author": "Konstantin Dergachev",
  "keywords": [
    "blog",
    "formula"
  ],
  "license": "MIT",
  "scripts": {
    "grab": "node app/server/graber/graber.js",
    "scss": "node-sass app/scss/main.scss app/public/css/style.css --output-style compressed --watch",
    "dev": "nodemon app/server.js -e pug,js,scss,css",
    "watch": "concurrently --names \"dev,scss,grab\" \"npm run dev\" \"npm run scss\" \"npm run grab\"",
    "start": "node app/server.js"
  },
  "engine": {
    "node": "13.7.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.3",
    "cloudinary": "1.11.0",
    "connect-flash": "^0.1.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-messages": "^1.0.1",
    "express-session": "^1.17.0",
    "express-validator": "^4.3.0",
    "memorystore": "^1.6.1",
    "mongoose": "^5.8.11",
    "mongoose-lifecycle": "^1.0.0",
    "node-sass": "^4.11.0",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "slugs": "^0.1.3"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  }
}
